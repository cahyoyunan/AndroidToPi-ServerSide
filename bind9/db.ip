;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	androidtopi.com. root.androidtopi.com. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.androidtopi.com.
180	IN	PTR	mail.androidtopi.com.
